/*  Aluno:
    Aluno:
----------------------------------------
t=numero de testes(1 ≤ t ≤ 100)
n=numero total de cargas (1 ≤ n ≤ 10^5)
p=numero de cargas por motorista(1 ≤ p ≤ n)

*/
#include <stdio.h>
#include <stdlib.h>

struct entrada {
    int id;
    int a, l, c;
};

int *solucao(struct entrada *entradas, int n, int p)
{
    int *ret=(int*)malloc(p*sizeof(int));
    int i,k,aux,maior;

    for(i=0;i<n;i++){
        entradas[i].a=entradas[i].a*entradas[i].l*entradas[i].c;//calcula o volumee coloca o valor em a
    }
    for(i=0;i<p;i++){
        maior=0;
        for(k=0;k<n;k++){
            if(maior<entradas[k].a){
                maior=entradas[k].a;
                aux=k;
            }
            if(maior==entradas[k].a){
                if(entradas[aux].id>entradas[k].id){
                    aux=k;
                }
            }
        }
        ret[i]=entradas[aux].id;
        entradas[aux].a=0;
    }

    return ret;
}

int main()
{
    int *ret;
    int t, i, j;
    int T, N, P, ID, A, L, C;

    scanf("%d",&T);

    for(t=0;t<T;t++) {

        scanf("%d %d",&N,&P);

        struct entrada *entradas=(struct entrada*)malloc(N*sizeof(struct entrada));
        for (i = 0; i < N; i++) {
            scanf("%d %d %d %d", &entradas[i].id, &entradas[i].a, &entradas[i].l, &entradas[i].c);
        }

        ret = solucao(entradas, N, P);

        for (i = 0; i < P; i++)
            if (i + 1 < P)
                printf("%d ", ret[i]);
            else
                printf("%d\n", ret[i]);
    }

    return 0;
}
